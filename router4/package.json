{
  "name": "router4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --colors --config ./webpack/webpack.config.client.js",
    "build:server": "webpack --progress --colors --config ./webpack/webpack.config.server.js",
    "clean": "rimraf dist",
    "serve": "npm run build:client && webpack-dev-server --config ./webpack/webpack.config.client.js --content-base dist/ --open --progress --colors --port 1111",
    "start": "npm run build && node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "node-sass": "^4.5.0",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "webpack-node-externals": "^1.5.4"
  }
